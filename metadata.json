{
  "name": "spantree-java78",
  "version": "0.4.0",
  "author": "spantree",
  "summary": "Oracle Java78 Puppet Module",
  "license": "Apache 2.0",
  "source": "https://github.com/dhardiker/puppet-java78.git",
  "project_page": "https://github.com/dhardiker/puppet-java78",
  "issues_url": "https://github.com/dhardiker/puppet-java78/issues",
  "description": "This module manages Oracle Java7 & Java8. Tested on Ubuntu",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/apt",
      "version_range": ">= 1.0.1"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_range": ">= 4.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=3.2.0 <4.0.0"
    }
  ]
}
